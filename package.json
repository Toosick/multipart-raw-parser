{
  "name": "multipart-raw-parser",
  "description": "A Javascript multipart/<whatever> raw parser",
  "version": "0.0.0-semantically-released",
  "author": "Justin Brottes <justin.brottes@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/lucmerceron/react-size-fetcher#readme",
  "bugs": {
    "url": "https://github.com/Wing-eu/multipart-raw-parser/issues"
  },
  "keywords": [
    "parser",
    "raw",
    "multipart",
    "response",
    "content",
    "parsing",
    "data"
  ],
  "files": [
    "build",
    "README.md"
  ],
  "private": false,
  "main": "build/main.js",
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint src",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "dev": "nodemon -w src --exec \"babel-node src --presets es2016,latest\"",
    "start": "babel-node ./node_modules/.bin/webpack && node build/main.js",
